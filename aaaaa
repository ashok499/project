package demo;
import java.util.ArrayList;

import javax.swing.text.html.HTMLDocument.Iterator;
public class Arraylist {
	String name;
	int rollno;
	Arraylist(String name, int rollno)
	{
		System.out.println("rollno=\t"+rollno +"    " + "name=\t"+name);
	}
}






package demo;
import java.util.ArrayList;
import java.util.Iterator;
public class student {
public void aaa()
{
	ArrayList <Arraylist> bb=new <Arraylist> ArrayList();
	Arraylist obj1=new Arraylist("aa",1);
	Arraylist obj2=new Arraylist("bb",2);
	Arraylist obj3=new Arraylist("cc",3);
	Arraylist obj4=new Arraylist("dd",4);
	Iterator itr=bb.iterator();
	while(itr.hasNext())
	{
		System.out.println(itr.next());
	}
}
	public static void main(String[] args) {
		student obj=new student();
		obj.aaa();
	}

}
